.next/
out/
build/
dist/
*.tsbuildinfo

# Testing
coverage/
.nyc_output/

# Environment files
.env*
!.env.example

# Logs
*.log
logs/

# Database
*.db
*.sqlite

# OS files
.DS_Store
Thumbs.db

# IDE
.vscode/
